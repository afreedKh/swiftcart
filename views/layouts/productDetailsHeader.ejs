    <style>
        
    .hidden-element{
        display: none;
    }
   
    .stock-message {
        font-size: 1rem;
        color: #333;
    }

   
    .out-of-stock {
        color: red;
        font-size: 1.2rem;
        font-weight: bold;
    }

    
    .low-stock {
        color: orange;
        font-size: 1rem;
    }

    
    .in-stock {
        color: green;
        font-size: 1rem;
    }
   
    .fa-check-circle {
        font-size: 3rem;
        color: #28a745; /* Bootstrap success color */
    }

   
    .modal-body.text-center {
        text-align: center;
    }

   
    .modal-footer .btn {
        margin: 0 5px;
    }
    .quantity-btn {
    width: 40px;
    height: 40px;
    border-radius: 50%;
    background-color: rgb(107, 103, 103);
    color: white;
    font-size: 20px;
    display: flex;
    justify-content: center;
    align-items: center;
    border: none;
    cursor: pointer;
    margin: 0 10px;
}

.quantity-btn:hover {
    background-color: rgb(22, 20, 20);
}

.product-variable-quantity {
    display: flex;
    align-items: center;
}

.product-variable-quantity input {
    width: 50px;
    height: 40px;
    border: 1px solid #ededed;
    background-color: transparent;
    text-align: center;
    padding: 0;
    font-size: 16px;
    border-radius: 5px;
    margin: 0 10px;
}

.product-variable-quantity input:focus {
    outline: none;
    border-color: rgb(212, 76, 76);
}

.product-add-to-cart-btn {
    margin-left: 20px;
}

.btn-primary {
    background-color: rgb(76, 133, 212);
    border: none;
    padding: 10px 20px;
    color: white;
    font-size: 16px;
    border-radius: 5px;
    cursor: pointer;
}

.btn-primary:disabled {
    background-color: #ccc;
    cursor: not-allowed;
}






