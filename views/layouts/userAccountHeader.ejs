            margin-left: 10px;
            cursor: pointer;
        }
        .default-form-box input, .checkbox-default input, .input-radio input {
            flex: 2;
            margin-left: 10px;
        }
        .input-radio {
            display: flex;
            align-items: center;
        }
        .input-radio span {
            margin-right: 10px;
        }
        
