   <style>
    .address-card {
    border: 1px solid #ddd;
    padding: 15px;
    margin-bottom: 20px;
    position: relative;
}

.address-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 10px;
}

.address-type {
    background-color: #f0f0f0;
    padding: 5px 10px;
    border-radius: 3px;
    font-weight: bold;
    font-size: 12px;
}

.address-actions {
    position: relative;
}

.action-button {
    background: none;
    border: none;
    font-size: 18px;
    cursor: pointer;
}

.dropdown-content {
    display: none;
    position: absolute;
    right: 0;
    background-color: white;
    box-shadow: 0px 8px 16px 0px rgba(0, 0, 0, 0.2);
    z-index: 1;
}

.dropdown-content a {
    color: black;
    padding: 8px 16px;
    text-decoration: none;
    display: block;
}

.dropdown-content a:hover {
    background-color: #f1f1f1;
}

.action-button:focus + .dropdown-content,
.dropdown-content:hover {
    display: block;
}

.address-body {
    margin-top: 10px;
}

.address-body strong {
    display: block;
    margin-bottom: 5px;
}

.address-body p {
    margin: 0;
    line-height: 1.5;
}

</style>

