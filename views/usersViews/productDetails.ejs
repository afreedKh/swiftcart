    <!-- Start Product Details Section -->
    <div class="product-details-section">
        <div class="container">
            <div class="row">
                <div class="col-xl-5 col-lg-6">
                    <div class="product-details-gallery-area" data-aos="fade-up" data-aos-delay="0">
                        <!-- Start Large Image -->
                          <div class="product-large-image product-large-image-horaizontal swiper-container">
                            <div class="swiper-wrapper">
                                <input type="hidden" id="product_id" value="<%=product._id%>">  
                                <% if (user) { %>  <!-- Check if user exists -->
                                    <input type="hidden" id="userId" value="<%= user._id %>">
                                <% } else { %>
                                    <input type="hidden" id="userId" value="">
                                <% } %>
                                <%product.varients.forEach((variant, variantIndex)=>{%>
                                    <%variant.images.forEach((image, imageIndex)=>{%>
                                        <div class="product-image-large-image swiper-slide zoom-image-hover img-responsive">
                                            <img id="largeImage" 
                                                 src="<%=image.url%>" 
                                                 alt="" 
                                                 data-variant-id="<%=variant._id%>"
                                                 class="<%=(variantIndex === 0 && imageIndex === 0) ? 'active' : ''%>">
                                        </div>
                                    <%})%>
                                <%})%> 
                            </div>
                        </div>  



                        <!-- Start Thumbnail Image -->

                        <div class="product-image-thumb product-image-thumb-horizontal swiper-container pos-relative mt-5">
                            <div class="swiper-wrapper">
                                <%product.varients.forEach((variant)=>{%>
                                    <%variant.images.forEach((image,index)=>{%>
                                        <div class="product-image-thumb-single swiper-slide">
                                            <img class="img-fluid variant-image" 
                                                src="<%= image.url %>" 
                                                alt="<%= image.altText %>" 
                                                data-variant-id="<%= variant._id %>" 
                                                data-stock="<%= variant.stock %>">
                                                <input type="hidden" id="stocked" value="<%=product.varients[0].stock%>">
                                        </div>
                                    <%})%>
                                <%})%>
                            </div>
                            <!-- Add Arrows -->
                            <div class="gallery-thumb-arrow swiper-button-next"></div>
                            <div class="gallery-thumb-arrow swiper-button-prev"></div>
                        </div>





                        <!-- End Thumbnail Image -->
                    </div>
                </div>
                <div class="col-xl-7 col-lg-6">
                    <div class="product-details-content-area product-details--golden" data-aos="fade-up"
                        data-aos-delay="200">
                        <!-- Start  Product Details Text Area-->
                        <div class="product-details-text">
                            <h4 class="title"><%=product.name%></h4>
                            <div class="d-flex align-items-center">
                                <ul class="review-star">
                                    <li class="fill"><i class="ion-android-star"></i></li>
                                    <li class="fill"><i class="ion-android-star"></i></li>
                                    <li class="fill"><i class="ion-android-star"></i></li>
                                    <li class="fill"><i class="ion-android-star"></i></li>
                                    <li class="empty"><i class="ion-android-star"></i></li>
                                </ul>
                                <a href="#" class="customer-review ml-2">(customer review )</a>
                            </div>
                            <%if(product.discount){%>

                            
                            <div class="actualPrice" style="text-decoration: line-through;">Rs.<%=product.price%>.00</div>
                            <div class="price">Rs.<%= product.discountedPrice.toFixed(2) %>  <span style="color: blueviolet;font-family: math;">(Save <%= product.discount %>%)</span> </div>
                            
                            <% } else { %>
                                <div class="actualPrice" style="font-size: 28px;">Rs.<%=product.price%>.00</div>
                            <%}%>
                            <p><%=product.description%></p>
                        </div> <!-- End  Product Details Text Area-->
                        <!-- Start Product Variable Area -->
                        <div class="product-details-variable">
                            <h4 class="title">Available Options</h4>
                            <!-- Product Variable Single Item -->
                            <div class="variable-single-item">
                                <span id="stock-display" class="product-stock-in">
                                    
                                    
                                        <i class="ion-checkmark-circled"></i>
                                   

                                    <span id="stock-message" class="stock-message">
                                        <% 
                                            const stock = product.varients[0].stock;
                                            let stockMessage;
                                            let stockClass = '';
                                            
                                            if (stock === 0) {
                                                stockMessage = 'Out of Stock';
                                                stockClass = 'out-of-stock'; // Class for zero stock
                                            } else if (stock < 10) { // Adjust this threshold as needed
                                                stockMessage = `${stock} Available`;
                                                stockClass = 'low-stock'; // Class for low stock
                                            } else {
                                                stockMessage = `${stock} In Stock`;
                                                stockClass = 'in-stock'; // Class for normal stock
                                            } 
                                        %> 


                                       
                                        <span class="<%= stockClass %>"><%= stockMessage %></span>
                                    </span>
                                </span>
                                
                                    
                            <!-- Product Variable Single Item -->
                            <div class="variable-single-item">
                                <span>Color</span>
                                <div class="product-variable-color" id="color-options">
                                    <input type="hidden" id="colorCode" value='<%= JSON.stringify(product) %>'>
                                    <!-- Color options will be dynamically inserted here -->
                                </div>
                            </div>
                            

                            <!-- Product Variable Single Item -->
                           
                            <!-- Product Variable Single Item -->
                            <div class="d-flex align-items-center ">
                                <div class="variable-single-item ">
                                    <span>Quantity</span>
                                    <div class="product-variable-quantity">
                                        <button class="quantity-btn" onclick="decreaseQuantity()">-</button>
                                        <input id="quantity-input" min="1" max="5" value="1" >
                                        <button class="quantity-btn" onclick="increaseQuantity()">+</button>
                                    </div>
                                </div>
                            
                                <div class="product-add-to-cart-btn">
                                    <form action="/cart" method="post">
                                        <% let allOutOfStock = product.varients.every(variant => variant.stock === 0); %>
                                        <button type="submit" class="btn btn-primary <%= allOutOfStock ? 'disabled' : '' %>"
                                            <% if (allOutOfStock) { %> 
                                                disabled 
                                            <% } %>
                                        >+ Add To Cart</button>
                                    </form>
                                </div>
                            </div>
                            <!-- Start  Product Details Meta Area-->
                            <div class="product-details-meta mb-20">
                                <p id="quantityError" style="color: red;"></p>
                                <a href="#" class="icon-space-right" id="addToWishlist" data-product-id="<%= product._id %>">
                                    <i style="width: 30px;" class="icon-heart"></i>Add to wishlist
                                </a>
                                
                            </div>
                        </div> <!-- End Product Variable Area -->

                        <!-- Start  Product Details Catagories Area-->
                            <!-- End  Product Details Catagories Area-->


                        <!-- Start  Product Details Social Area-->
                        <div class="product-details-social" >
                            <span class="title">SHARE THIS PRODUCT:</span>
                            <ul>
                                <li><a href="https://www.facebook.com/"><i class="fab fa-facebook"></i></a></li>
                                <li><a href="https://x.com/"><i class="fab fa-twitter"></i></a></li>
                                <li><a href="https://in.pinterest.com/  "><i class="fab fa-pinterest"></i></a></li>
                                <li><a href="https://support.google.com/answer/2451065?hl=en"><i class="fab fa-google-plus"></i></a></li>
                                <li><a href="https://in.linkedin.com/"><i class="fab fa-linkedin"></i></a></li>
                            </ul>
                        </div> 
                        <!-- End  Product Details Social Area-->
                    </div>
                </div>
            </div>
        </div>
    </div> <!-- End Product Details Section -->

