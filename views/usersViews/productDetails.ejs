<script>
    document.addEventListener('DOMContentLoaded', function() {
        const colorOptionsContainer = document.getElementById('color-options');
        const productElement = document.getElementById("colorCode").value;

        // Parse the product JSON from the hidden input
        let product;
        try {
            product = JSON.parse(productElement);
        } catch (error) {
            console.error('Failed to parse product JSON:', error);
            return; // Exit if parsing fails
        }

        // Check if the product has variants
        if (product && Array.isArray(product.varients)) {
            product.varients.forEach((variant, index) => {
                // Ensure variant has a color object
                if (variant.color) {
                    const colorCode = variant.color.colorCode || 'No colorCode';
                    const colorName = variant.color.color || 'No color';
                    console.log(colorCode, colorName);

                    // Create label and input elements for color selection
                    const label = document.createElement('label');
                    label.htmlFor = `product-color-${index}`;

                    const input = document.createElement('input');
                    input.name = 'product-color';
                    input.id = `product-color-${index}`;
                    input.className = 'color-select';
                    input.type = 'radio';
                    if (index === 0) input.checked = true; // Check the first option by default

                    const span = document.createElement('span');
                    span.className = 'product-color-custom';
                    span.style.backgroundColor = colorCode; // Set color preview

                    // Append elements
                    label.appendChild(input);
                    label.appendChild(span);
                    colorOptionsContainer.appendChild(label);
                } else {
                    console.log('Color not found for variant:', index);
                }
            });
        } else {
            console.error('Product or variants array is undefined.');
        }
    });
</script>

<script>
    document.addEventListener('DOMContentLoaded', function() {
        var menuToggle = document.querySelector('a[href="#mobile-menu-offcanvas"]');
        var mobileMenu = document.getElementById('mobile-menu-offcanvas');
        var closeButton = mobileMenu.querySelector('.offcanvas-close');
    
        menuToggle.addEventListener('click', function(e) {
            e.preventDefault();
            mobileMenu.classList.add('offcanvas-open');
        });
    
        closeButton.addEventListener('click', function() {
            mobileMenu.classList.remove('offcanvas-open');
        });
    
        // Close menu when clicking outside
        document.addEventListener('click', function(e) {
            if (!mobileMenu.contains(e.target) && !menuToggle.contains(e.target)) {
                mobileMenu.classList.remove('offcanvas-open');
            }
        });
    });
    </script>


