        <script>

            document.addEventListener('DOMContentLoaded', function() {
            let activeVariantId = '';
            let initialStock =  document.getElementById("stocked").value;
        
   
            const thumbnailImages = document.querySelectorAll('.variant-image');
            thumbnailImages.forEach(img => {
                img.addEventListener('click', function() {
            activeVariantId = this.getAttribute('data-variant-id');
            
            
            const mainImage = document.querySelector('#largeImage');
            mainImage.src = this.src;
            mainImage.setAttribute('data-variant-id', activeVariantId);
            
            
            const stock = parseInt(this.getAttribute('data-stock'), 10);
            initialStock = stock; 
            updateStockDisplay(stock);

            
        });
    });
    
    // Function to update stock display
    function updateStockDisplay(stock) {
       
        const quantity = document.querySelector('.product-variable-quantity input').value;
        const stockDisplay = document.getElementById('stock-message');
        let stockMessage;
        let stockClass = '';
        
        if (stock == 0) {
            stockMessage = 'Out of Stock';
            stockClass = 'out-of-stock';
        } else if (stock < 10) {
            stockMessage = `${stock} Available`;
            stockClass = 'low-stock';
        } else {
            stockMessage = `${stock} In Stock`;
            stockClass = 'in-stock';
        }

        
        stockDisplay.textContent = stockMessage;
        stockDisplay.className = stockClass;
    }




    // Handle add to cart
    function addToCart(event) {
        event.preventDefault();
       
        const productId = document.getElementById("product_id").value;
        const userId = document.getElementById('userId').value;
        const varientId = activeVariantId || document.querySelector('#largeImage').getAttribute('data-variant-id');
        const quantity = document.querySelector('.product-variable-quantity input').value;


        document.getElementById("quantityError").innerText=""
     
        let quantities=Number(quantity)
        let stockes = Number(initialStock)

        if(quantities>stockes){
           return document.getElementById("quantityError").innerText=`Only ${stockes} units are available in stock.`
        }else if(quantities>5){
            return document.getElementById("quantityError").innerText='Please note: You can only add up to 5 units of this item to your cart.'

        }
        
       




        if(!userId){
            window.location.href="/login"
        }

        fetch('/cart', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({ userId, productId, varientId, quantity })
        })
        .then(response => response.json())
        .then(data => {
           
            if (data.success) {
                // alert('Item added to cart!');
                new bootstrap.Modal(document.getElementById('modalAddcart')).show();
            }
        })
        .catch(err => {
            console.error('Error:', err);
        });
    }

    // Attach event listener to the form
    const form = document.querySelector('.product-add-to-cart-btn form');
    if (form) {
        form.addEventListener('submit', addToCart);
    }

    // Set initial active variant ID
    const initialImage = document.querySelector('#largeImage');
    if (initialImage) {
        activeVariantId = initialImage.getAttribute('data-variant-id');
    }

    });


    </script>



